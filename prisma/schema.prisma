generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid()) @db.Uuid()
  name     String
  email    String @unique
  password String
  level    Int    @default(1)

  @@map("users")
}
